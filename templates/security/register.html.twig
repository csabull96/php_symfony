{% extends 'base.html.twig' %}

{% block title %}Register{% endblock %}

{% block body %}
    {% if form is not null and form is not empty %}
        <div class="col-5">
            {% form_theme form 'bootstrap_4_layout.html.twig' %}
            {{ form_start(form) }}
            {{ form_widget(form) }}
            {{ form_end(form) }}
        </div>
    {% endif %}

{% endblock %}